                android:layout_marginTop="@dimen/loginViewsMargin"
                android:layout_weight="0"
                android:gravity="center_horizontal|center_vertical"
                android:text="EAV Collecte:"
                android:textSize="18dp" />

            <Spinner
                android:id="@+id/spinner_default_eav_for_collecte"

                style="@style/parent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/loginViewsMargin"
                android:layout_weight="1" />

        </LinearLayout>
        <!-- GxIsPreComGesGuPGC /A1** Pour Préciser si il existe Président de Gestion de Guichet /PGG		::Q -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/lblGxIsPreComGesGuPGC"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Existe t'il un Président de Gestion de Guichet ?"
                android:textSize="18dp" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/SwitchGxIsPreComGesGuPGC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:checked="false"
                android:textOff="Non"
                android:textOn="Oui"
                app:showText="true" />

        </LinearLayout>
        <!-- GxIsChefGuichCG /A1	** Pour Préciser si il existe un Chef de Guichet /CG		::R -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/lblGxIsChefGuichCG"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Existe t'il un Chef de Guichet ?"
                android:textSize="18dp" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/SwitchGxIsChefGuichCG"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:checked="false"
                android:textOff="Non"
                android:textOn="Oui"
                app:showText="true" />

        </LinearLayout>
        <!-- GxIsAgentGuichAG /A1	** Pour préciser si il existe des Agents de Guichet, toujours activé /Y		::P -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/lblGxIsAgentGuichAG"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Existe t'il des Agents de Guichet ?"
                android:textSize="18dp" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/SwitchGxIsAgentGuichAG"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:checked="true"
                android:textOff="Non"
                android:textOn="Oui"
                app:showText="true" />

        </LinearLayout>
        <!-- GxMtPlafDecAutPCG /R	** Pour Préciser le montant plafond autorisé à gérer ou valider par le PCG -->
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/input_layout_GxMtPlafDecAutPCG"
            style="@style/parent"
            android:layout_marginTop="@dimen/loginViewsMargin">

            <EditText
                android:id="@+id/input_txt_GxMtPlafDecAutPCG"
                style="@style/modifiedEditText"
                android:hint="Mt plafond à gérer par le PCG"
                android:inputType="numberDecimal"
                android:maxLines="1"
                android:textSize="16sp"
                android:textStyle="bold" />
        </com.google.android.material.textfield.TextInputLayout>
        <!-- GxNbMoisHistoDef /I	* Pour renseigner le nombre de mois par défaut des Historiques du Guichet en eploitation, 2 par défaut. -->
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/input_layout_GxNbMoisHistoDef"
            style="@style/parent"
            android:layout_marginTop="@dimen/loginViewsMargin">

            <EditText
                android:id="@+id/input_txt_GxNbMoisHistoDef"
                style="@style/modifiedEditText"
                android:hint="Nbre mois historique par defaut"
                android:inputType="number"
                android:maxLines="1"
                android:text="2"
                android:textSize="16dp"
                android:textStyle="bold" />
        </com.google.android.material.textfield.TextInputLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/input_txt_email_user"
            android:orientation="vertical"

            android:paddingTop="10dp">

            <br.com.simplepass.loadingbutton.customViews.CircularProgressButton
                android:id="@+id/btn_save_guichet"
                style="@style/loginButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginRight="12dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1"
                android:padding="12dp"
                android:text="@string/btn_enregistrer" />

            <br.com.simplepass.loadingbutton.customViews.CircularProgressButton
                android:id="@+id/btn_clean"
                style="@style/loginButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1"
                android:padding="12dp"
                android:text="@string/btn_annuler" />

            <br.com.simplepass.loadingbutton.customViews.CircularProgressButton
                android:id="@+id/btn_delete_guichet"
                style="@style/cancelButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1"

                android:padding="12dp"
                android:text="@string/btn_supprimer" />

        </LinearLayout>
